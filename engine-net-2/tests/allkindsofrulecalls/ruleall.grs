new graph "allkindsofrulecalls/rules"

dump set node N color red
dump add node N shortinfotag i
dump set node M color blue

exec init

exec %r

clear graph

exec init

exec [%r]

clear graph

exec init

exec [%r\orderDescendingBy<i>\keepFirst(2)]

clear graph

exec init

exec count[%r] => ::c

clear graph

exec init

exec $%[%r]

clear graph

exec init

exec ::v=2 ;> $%::v[%r]

#clear graph

#exec init

#debug exec ::v=4 ;> $%::v[%r]

#clear graph

#exec init

#debug exec ::v=2 ;> ::w=2 ;> $%::v,::w[%r]

quit
