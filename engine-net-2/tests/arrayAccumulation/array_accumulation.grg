rule sum : (int, int) {
---
	def var x:int;
	
	yield { 
		yield x = array<int>[1,2,3].sum();
	}
	
	modify {
	---
		def var y:int;

		eval {
			yield y = array<int>[1,2,3].sum();
		}
		
		return(x,y);
	}
}

