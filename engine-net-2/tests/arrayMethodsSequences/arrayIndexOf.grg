rule indexOf(ref a:array<int>) : (int)
{
	modify {
	---
		def var o:int;
		exec({yield o=a.indexOf(42)});
		return(o);
	}
}

rule indexOfString(ref a:array<string>) : (int)
{
	modify {
	---
		def var o:int;
		exec({yield o=a.indexOf("a42")});
		return(o);
	}
}

