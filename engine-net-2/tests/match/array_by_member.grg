test Simple(var i:int, var j:int)
{
}

rule aindexofby(ref a:array<match<Simple>>) : (int)
{
	if{ a.indexOfBy<i>(1) == 1; }
	
	modify {
		return(a.indexOfBy<i>(1));
	}
}

